/* pages/collectInformation/index.wxss */
.guideList-page{
  width: 100%;
  height: 100%;
  background: #42c6cb;
}
.guide-header-box{
  width: 100%;
  position: relative;
  z-index: 99;
}
.guide-header-box image{
  width: 100%;
  z-index: 99;
}
.header-title{
  font-size: 36rpx;
  font-family: PingFang, PingFang-SC-Bold;
  font-weight: SC-Bold;
  text-align: left;
  color: #ffffff;
  width: 100%;
  height: 10vh;
  padding-top: 6vh;
  padding-left: 4vw;
  box-sizing: border-box;
}
.header-title image{
  width: 18rpx;
  height: 30rpx;
  margin-right: 30rpx;
  display: inline-block;
}
.contentext1{
  width: 750rpx;
  height: auto;
  background: #eefeff;
  border-radius: 16rpx 16rpx 0px 0px;
  margin-top: -600rpx;
  z-index: 100;
  position: relative;
  padding-top: 50rpx;
  padding-bottom: 150rpx;
  box-sizing: border-box;
  text-align: center;
} 
